----+    if (this.approvalWorkflow.isRequired(toMode)) {
----+      const approval = await this.approvalWorkflow.request(toMode, evidence);
----+      if (!approval.granted) {
----+        return {
----+          approved: false,
----+          reason: 'Approval denied'
----+        };
----+      }
----+    }
----+    
----+    // 4. Execute transition
----+    return this._executeTransition(fromMode, toMode);
