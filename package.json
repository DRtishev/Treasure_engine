{
  "name": "treasure-engine",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "ws": "^8.18.0"
  },
  "scripts": {
    "verify:truth": "bash scripts/verify/truth.sh",
    "verify:hacks": "node scripts/verify/hacks_validate.mjs hacks/hack_pack_v1.json",
    "verify:court": "node scripts/verify/court_stub.mjs && node scripts/verify/json_schema_check.mjs truth/court_report.schema.json reports/court_report.json",
    "verify:all": "npm run verify:all-data && npm run verify:treasure",
    "sim:data": "node core/sim/bar_generator.mjs",
    "sim:all": "npm run sim:data && node core/sim/engine.mjs",
    "court:v1": "node core/court/court_v1.mjs",
    "court:v2": "node core/court/court_v2.mjs",
    "verify:sim-schema": "node scripts/verify/json_schema_check.mjs truth/sim_report.schema.json reports",
    "verify:eqs-schema": "node scripts/verify/json_schema_check.mjs truth/eqs_report.schema.json reports/eqs_report.json",
    "verify:e2": "node scripts/verify/run_with_context.mjs --gate e2 --seed ${SEED:-12345} --repeat default -- npm run verify:e2:raw",
    "verify:phase2": "node scripts/verify/phase2_verify.mjs",
    "verify:dataset": "node scripts/verify/dataset_check.mjs",
    "verify:binance": "node scripts/verify/binance_smoke.mjs",
    "verify:websocket": "node scripts/verify/websocket_smoke.mjs",
    "verify:confidence": "node scripts/verify/confidence_score.test.mjs",
    "verify:agent": "node scripts/verify/agent_smoke.mjs",
    "verify:treasure": "node scripts/profit/__tests__/treasure_engine.test.mjs",
    "verify:paper": "node scripts/verify/run_with_context.mjs --gate paper --seed ${SEED:-12345} --repeat default -- npm run verify:paper:raw",
    "verify:live": "node scripts/verify/live_adapter_smoke.mjs",
    "verify:adversarial": "node scripts/verify/adversarial_safety.mjs",
    "verify:godtier": "node scripts/verify/godtier_integration.mjs",
    "verify:truth-layer": "node scripts/verify/truth_layer_check.mjs",
    "verify:epoch06": "npm run verify:e2 && npm run verify:truth-layer",
    "verify:epoch07": "node scripts/verify/epoch07_integration.mjs",
    "verify:epoch08": "node scripts/verify/epoch08_integration.mjs",
    "verify:epoch09": "node scripts/verify/epoch09_integration.mjs",
    "verify:epochX": "node scripts/verify/epochX_integration.mjs",
    "verify:reality-gap": "node scripts/verify/verify_reality_gap.mjs",
    "verify:all-data": "npm run verify:binance && npm run verify:websocket && npm run verify:confidence && npm run verify:agent",
    "data:import:csv": "node scripts/data/import_ohlcv_csv.mjs",
    "data:import:binance": "node scripts/data/import_binance_klines_json.mjs",
    "data:fetch:binance": "node scripts/data/fetch_binance_history.mjs",
    "demo:live": "node scripts/data/live_price_demo.mjs",
    "demo:agent": "node scripts/data/agent_demo.mjs",
    "hunt": "node scripts/profit/treasure_map_demo.mjs",
    "verify:epoch10": "node scripts/verify/epoch10_integration.mjs",
    "verify:epoch11": "node scripts/verify/epoch11_master_orchestrator.mjs",
    "verify:persist": "node scripts/verify/persist_idempotency.mjs",
    "verify:config": "node scripts/verify/config_check.mjs",
    "verify:determinism": "node scripts/verify/determinism.mjs",
    "gate:determinism": "npm run verify:determinism",
    "verify:dryrun-live": "node scripts/verify/dryrun_live_e2e.mjs",
    "gate:exec": "npm run verify:dryrun-live",
    "verify:events": "node scripts/verify/events_schema_check.mjs",
    "gate:obs": "npm run verify:events",
    "verify:integration": "node scripts/verify/master_integration_e2e.mjs",
    "gate:integration": "npm run verify:integration",
    "verify:safety": "node scripts/verify/safety_gates_check.mjs",
    "verify:e2:raw": "npm run verify:hacks && npm run verify:truth || true && npm run sim:all && npm run verify:sim-schema && npm run verify:eqs-schema && npm run court:v1 && node scripts/verify/json_schema_check.mjs truth/court_report.schema.json reports/court_report.json && node ui/generate_panel.mjs",
    "verify:e2:multi": "node scripts/verify/e2_multi_seed.mjs",
    "verify:core": "npm run verify:e2 && npm run verify:paper && npm run verify:e2:multi",
    "verify:paper:raw": "node scripts/verify/paper_e2e.mjs"
  },
  "devDependencies": {
    "ajv": "^8.17.1"
  }
}
